{
  "name": "ide-typescript",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "TypeScript and Javascript language support for Atom-IDE",
  "repository": "https://github.com/atom/ide-typescript",
  "license": "MIT",
  "engines": {
    "atom": ">=1.17.0"
  },
  "dependencies": {
    "atom-languageclient": "^0.1.0",
    "javascript-typescript-langserver": "^2.0.0-pre.34"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "nuclide-datatip.provider": {
      "versions": {
        "0.0.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "nuclide-code-format.provider": {
      "versions": {
        "0.0.0": "provideCodeFormat"
      }
    },
    "nuclide-definition-provider": {
      "versions": {
        "0.0.0": "provideDefinitions"
      }
    },
    "nuclide-find-references.provider": {
      "versions": {
        "0.0.0": "provideFindReferences"
      }
    },
    "nuclide-outline-view": {
      "versions": {
        "0.0.0": "provideOutlines"
      }
    }
  }
}
